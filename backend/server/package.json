{
  "name": "discordbottest1.0",
  "version": "1.0.0",
  "description": "discordbottest",
  "main": "Bot.js",
  "scripts": {
    "test": "start",
    "start": "ts-node ./src/index.ts",
    "deploy-commands": "ts-node ./src/deploy-commands.ts",
    "migrate:generate": "npm run typeorm migration:generate src/migrations/vdr",
    "migrate:up": "npm run typeorm migration:run",
    "migrate:down": "npm run typeorm migration:revert",
    "typeorm": "env-cmd ts-node ./node_modules/typeorm/cli.js -d src/config/database.ts"
  },
  "author": "Cherinas",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^1.6.1",
    "@discordjs/rest": "^1.7.0",
    "@discordjs/voice": "^0.16.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "discord.js": "^14.9.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "g": "^2.0.1",
    "moment": "^2.29.4",
    "nodemon": "^2.0.22",
    "sqlite3": "^5.1.6",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.15"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17"
  }
}
